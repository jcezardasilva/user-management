{
    "tags": {
        "allowUnknownTags": false
    },
        "source": {
            "include": "api/",
            "includePattern": ".+(controller|model|schema)\\.js$",
            "excludePattern": "(^|\\/|\\\\)_"
        },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/tui-jsdoc-template",
        "encoding": "utf8",
        "destination": "public/docs/",
        "recurse": true,
        "verbose": true
    }
}